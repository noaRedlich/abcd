<?xml version="1.0" encoding="utf-8"?>
<xml>
	<main_table>listingsStocks</main_table>
	<name>#suppliers#</name>
	<options>
		<opt name="create_record" type="always"></opt>
		<opt name="search" type="sometimes"></opt>
		<opt name="main_view" type="always"></opt>
		<opt name="delete_record" type="none"></opt>
	</options>
	<view>
		<vcol name="SortOrder" >#admin_template_editor_field_rank#</vcol>
		<vcol name="StockName">#user_name#</vcol>
		<vcol name="TerminalID">#term_code#</vcol>
		<vcol name="UpdateTime">#upd_time#</vcol>
		<vcol name="Status" type="yes_no">#admin_listings_editor_active#</vcol>
	</view>
	<edit>
		<ecol name="StockName" type="string">#user_name#</ecol>
		<ecol name="SortOrder"  type="string">#admin_template_editor_field_rank#</ecol>
		<ecol name="TerminalID"  type="string">#term_code#</ecol>
		<ecol name="UpdateTime"   type="string">#upd_time#</ecol>
		<ecol name="Status" type="yes_no">#admin_listings_editor_active#</ecol>
	</edit>
	<sql_actions>
		<opt name="order" val="SortOrder,StockName"></opt>
		<opt name="search_by" val="StockName,TerminalID"></opt>
	</sql_actions>
</xml>
