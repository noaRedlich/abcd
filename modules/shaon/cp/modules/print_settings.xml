<?xml version="1.0" encoding="utf-8"?>
<xml>
	<main_table>userdata</main_table>
	<name>#users_pref#</name>
	<options>
		<opt name="create_record" type="none"></opt>
		<opt name="search" type="none"></opt>
		<opt name="main_view" type="always"></opt>
		<opt name="delete_record" type="none"></opt>
	</options>
	<view>
		<vcol name="UserName" >#username#</vcol>
	</view>
	
	<edit>

		
		<ecol name="print_ashray_z" type="yes_no">הדפס אשרא</ecol>
		<ecol name="print_prepaid_z" type="yes_no">הדפס כרטיס מתנה</ecol>
		<ecol name="print_zicuy_out_z" type="yes_no">הדפס פתק זיכוי יוצא</ecol>
		<ecol name="print_zicuy_in_z" type="yes_no">הדפס פתק זיכוי נכנס</ecol>
		
		<ecol name="print_hakafa_z" type="yes_no">הדפס  הקפה</ecol>
		<ecol name="print_shovar_z" type="yes_no">הדפס שובר</ecol>
		<ecol name="print_cheque_z" type="yes_no">הדפס המחאה</ecol>
		<ecol name="print_cash_z" type="yes_no">הדפס מזומן</ecol>
		<ecol name="print_date_z" type="yes_no">הדפס תאריך</ecol>
		
		
	</edit>
	<sql_actions>
		<opt name="order" val="UserName"></opt>
		<opt name="search_by" type="UserName"></opt>
	</sql_actions>
</xml>