<?xml version="1.0" encoding="windows-1255"?>
<xml>
  <main_table>userdata</main_table>
  <name>הגדרות משתמש</name>
  <options>
    <opt name="create_record" type="none"></opt>
    <opt name="search" type="none"></opt>
    <opt name="main_view" type="always"></opt>
    <opt name="delete_record" type="none"></opt>
  </options>
  <view>
    <vcol name="UserName" >שם משתמש</vcol>
  </view>
  <edit>
    <ecol tab="1" name="listings_per_page" type="enum">
      מספר רשומות בכל דף
      <enum val="10">10</enum>
      <enum val="13">13 (תואם לדף)</enum>
      <enum val="15">15</enum>
      <enum val="20">20</enum>
      <enum val="25">25</enum>
      <enum val="30">30</enum>
      <enum val="35">35</enum>
      <enum val="40">40</enum>
      <enum val="45">45</enum>
      <enum val="50">50</enum>
    </ecol>
    <ecol tab="1" name="visual_effects" type="yes_no">אפקט ויזואלי בהעברת העמוד</ecol>
    <ecol tab="1" name="RemindBackup" type="yes_no">תזכורת גיבוים</ecol>
    <ecol tab="1" name="LogoPicture" type="file" targetfilename="logo" allowedtypes="jpeg,jpg,png" targetdir="_MlaitekPro">תמונת לוגו</ecol>
    <ecol tab="1" name="ShowDocTotalQuantity" type="yes_no">להציג סה''כ פריטים במסמך</ecol>
    <ecol tab="1" name="EnableStockOverdraft" type="yes_no">לאפשר חריגת כמויות בהעברות</ecol>
    <ecol tab="2" name="PrintKlitaAsmachta" type="yes_no">להדפיס אסמכתות של מסמכי מקור</ecol>
    <ecol tab="2" name="PrintImmediately" type="yes_no" defaultvalue="1">להדפיס  מסמכים בקופה באופן מיידי</ecol>
  </edit>
  <tabs>
    <tab index="1" name="כללי"></tab>
    <tab index="2" name="הדפסה"></tab>
  </tabs>
  <sql_actions>
    <opt name="order" val="UserName"></opt>
    <opt name="search_by" type="UserName"></opt>
  </sql_actions>
</xml>