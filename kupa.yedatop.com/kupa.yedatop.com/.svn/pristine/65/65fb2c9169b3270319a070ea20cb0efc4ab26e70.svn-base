<?xml version="1.0" encoding="windows-1255"?>
<xml>
  <main_table>currencies</main_table>
  <name>מטבע חול</name>
  <options>
    <opt name="create_record" type="none"></opt>
    <opt name="edit_record" type="sometime"></opt>
    <opt name="search" type="none"></opt>
    <opt name="view" type="always"></opt>
    <opt name="delete_record" type="none"></opt>
  </options>
  <view>
    <vcol name="SortOrder">#</vcol>
    <vcol name="name">שם</vcol>
    <vcol name="code">קוד</vcol>
    <vcol name="course">שער</vcol>
  </view>
  <edit>
    <ecol name="name" type="string">שם</ecol>
    <ecol name="code" type="string">קוד</ecol>
    <ecol name="symbolbefore" type="string" maxlength="1">סמל לפני סכום</ecol>
    <ecol name="symbolafter" type="string" maxlength="3">סמל אחרי סכום</ecol>
    <ecol name="course" type="string">שער לעומת מטבע ראשי</ecol>
  </edit>
  <sql_actions>
    <opt name="order" val="SortOrder"></opt>
  </sql_actions>
</xml>
