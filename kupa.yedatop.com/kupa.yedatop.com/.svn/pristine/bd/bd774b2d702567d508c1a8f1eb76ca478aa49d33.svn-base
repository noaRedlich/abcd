	function sum($rs,$field,&$ids){
		$s=0;
		$ids="";
		$rs->MoveFirst();
		while(!$rs->EOF){
			$s+=$rs->fields[$field];
			if (!strpos(" ".$ids,",".$rs->fields["ID"])){
				$ids.=",".$rs->fields["ID"];
			}
			$rs->MoveNext();
		}
		$ids = substr($ids,1);
		return $s;
	}
